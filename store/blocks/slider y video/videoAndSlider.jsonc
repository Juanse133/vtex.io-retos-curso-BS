    "slider-layout#banner-video": {
        "props": {
            "itemsPerPage": {
                "desktop": 1,
                "tablet": 1,
                "phone": 1
            },
            "showNavigationArrows": "always",
            "showPaginationDots": "always",
            "infinite": false,
            "autoplay": {
                "timeout": 3500
            },
            "fullWidth": true
