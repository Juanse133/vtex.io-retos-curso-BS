            "href": "/video-prueba"
        },
        "children": ["video#banner-video"]
    },
    "video#banner-video": {
        "props": {
            "width": "100%",
            "height": "100%",
            "loop": true,
            "autoPlay": true,
            "muted": true,
            "controlsType": "none",
